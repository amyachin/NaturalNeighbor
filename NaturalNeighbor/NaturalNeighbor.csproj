<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net472;netstandard2.1</TargetFrameworks>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>$(SolutionDir)NaturalNeighbor.snk</AssemblyOriginatorKeyFile>
    <Authors>Alexander Myachin</Authors>
    <Company />
    <Description>Natural Neighbor 2D Interpolator based on Delanuay tesselation</Description>
    <PackageProjectUrl></PackageProjectUrl>
    <PackageIcon>NN.png</PackageIcon>
    <PackageIconUrl />
    <RepositoryUrl>https://github.com/amyachin/NaturalNeighbor</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>voronoi interpolation GIS</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="GitVersion.MsBuild" Version="5.8.1">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
    <PackageReference Include="System.Collections.Immutable" Version="6.0.0" />
    <PackageReference Include="System.Numerics.Vectors" Version="4.5.0" />
  </ItemGroup>

<ItemGroup>
    <InternalsVisibleTo Include="NaturalNeighbor.UnitTests, " />
</ItemGroup>

<ItemGroup>
  <None Include="NN.png">
    <Pack>True</Pack>
    <PackagePath></PackagePath>
  </None>
</ItemGroup>


</Project>
